{
    "metadata": [
      {
        "src": [
          {
            "files": [
              "dropbox-sdk-dotnet/Dropbox.Api/**.csproj"
            ],
            "cwd": ".",
            "exclude": [
              "**/obj/**",
              "**/bin/**",
              "_site/**"
            ]
          }
        ],
        "dest": "obj/api",
        "properties": {
            "TargetFramework": "netstandard2.0"
        }
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "api/**.yml"
          ],
          "cwd": "obj"
        },
        {
          "files": [
            "*.md"
          ],
          "exclude": [
            "obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "_site",
      "template": [
        "default"
      ]
    }
  }