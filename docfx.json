{
    "metadata": [
      {
        "src": [
          {
            "files": [
              "dropbox-sdk-dotnet/Dropbox.Api/**.csproj"
            ],
            "exclude": [
              "**/obj/**",
              "**/bin/**",
              "_site/**"
            ],
            "src": "../"
          }
        ],
        "dest": "obj/api",
        "properties": {
            "TargetFramework": "net45"
        }
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "obj/api/**.yml"
          ]
        },
        {
          "files": [
            "*.md", "toc.yml"
          ],
          "exclude": [
            "_site/**"
          ]
        }
      ],
      "dest": "_site",
      "template": [
        "default"
      ]
  }
}